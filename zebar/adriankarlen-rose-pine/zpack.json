{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.1/resources/zpack-schema.json",
  "name": "adriankarlen-rose-pine",
  "version": "1.0.0", 
  "description": "Adrian Karlen's Rose Pine theme adapted for modern Zebar with proper window management",
  "tags": ["rose-pine", "dark", "aesthetic", "rounded"],
  "previewImages": [],
  "repositoryUrl": "https://github.com/adriankarlen/rose-pine.zebar",
  "widgets": [
    {
      "name": "bar",
      "htmlPath": "./bar.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": false,
      "transparent": true,
      "includeFiles": [
        "**/**"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_center",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "60px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": true,
            "edge": "top",
            "windowMargin": "0px"
          }
        }
      ]
    }
  ]
}